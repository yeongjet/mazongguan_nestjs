{
  "name": "@mazongguan/bp",
  "version": "0.0.1",
  "scripts": {
    "build": "babel --extensions .ts --ignore node_modules dist . -d ./dist",
    "build2": "rimraf dist && babel --extensions .ts --ignore node_modules dist . -d ./dist",
    "start:dist": "babel-node dist/src/main.js",
    "start:ts-node": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:babel-node": "babel-node --extensions .ts --config-file ./babel.config.js -- src/main.ts --debug",
    "test": "jest"
  },
  "dependencies": {
  }
}
